    "# poslist = ['JJ', 'JJR', 'JJS']\n",
    "\n",
    "\n",
    "test_data = [(features_from_text(text,poslist),cl) for text,cl in test_data_raw]\n",
    "train_data = [(features_from_text(text,poslist),cl) for text,cl in train_data_raw]"
   ]
